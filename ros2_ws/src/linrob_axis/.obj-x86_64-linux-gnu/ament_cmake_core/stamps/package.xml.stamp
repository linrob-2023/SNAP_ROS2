<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>linrob_axis</name>
  <version>0.1.2</version>
  <description>Position controller responsible for communication between Datalayer and PLC.</description>
  <maintainer email="info@matom.ai">Matomai</maintainer>
  <license>proprietary</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>std_msgs</depend>
  <depend>controller_interface</depend>
  <depend>pluginlib</depend>
  <build_depend>ros2_control</build_depend>
  <build_depend>hardware_interface</build_depend>
  <build_depend>rosidl_default_generators</build_depend>

  <exec_depend>ros2_control</exec_depend>
  <exec_depend>hardware_interface</exec_depend>
  <exec_depend>rosidl_default_runtime</exec_depend>


  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <hardware_interface_plugin>resource_plugin.xml</hardware_interface_plugin>
    <controller_interface_plugin>controller_plugin.xml</controller_interface_plugin>
  </export>

  <member_of_group>rosidl_interface_packages</member_of_group>
</package>
